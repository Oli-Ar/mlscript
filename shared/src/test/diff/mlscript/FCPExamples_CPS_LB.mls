// (* A function that receives a token *)
def f t = (s x x t x x e)
//│ f: (() -> (forall 'b, 'a. (List["x" | 'a]
//│   where
//│     'b <: List['a])) -> (() -> (forall 'c. 'c -> (() -> (forall 'c, 'a0. (List["x" | 'a0]
//│   where
//│     'c <: List['a0])) -> 'd) -> 'd)) -> (() -> (forall 'c. 'c -> (() -> (forall 'c, 'a0. (List["x" | 'a0]
//│   where
//│     'c <: List['a0])) -> 'd) -> 'd)) -> (() -> List[string] -> () -> (forall 'e. 'e -> (() -> (forall 'e, 'a1. (List['a1 | string]
//│   where
//│     'e <: List['a1])) -> 'f) -> 'f)) -> 'g) -> 'g

// (* If the token is used twice, we must reveive two arguments *)
def g t1 t2 = (s x x t1 x (s x t2 x e) e)
//│ g: 'b -> (forall 'b. ('c -> 'd
//│   where
//│     'b <: () -> (forall 'e, 'a. (List["x" | 'a]
//│   where
//│     'e <: List['a])) -> (() -> (forall 'f. 'f -> (() -> (forall 'f, 'a0. (List["x" | 'a0]
//│   where
//│     'f <: List['a0])) -> 'g) -> 'g)) -> (forall 'c. ('h
//│   where
//│     'c <: () -> (forall 'i, 'a1. (List["x" | 'a1]
//│   where
//│     'i <: List['a1])) -> (() -> (forall 'f. 'f -> (() -> (forall 'f, 'a0. (List["x" | 'a0]
//│   where
//│     'f <: List['a0])) -> 'g) -> 'g)) -> (() -> List[string] -> () -> (forall 'j. 'j -> (() -> (forall 'j, 'a2. (List['a2 | string]
//│   where
//│     'j <: List['a2])) -> 'k) -> 'k)) -> 'h)) -> (() -> List[string] -> () -> (forall 'j. 'j -> (() -> (forall 'j, 'a2. (List['a2 | string]
//│   where
//│     'j <: List['a2])) -> 'k) -> 'k)) -> 'd))

// (* This does not type. It requires first-class polymorphism. *)
def h t = g t t
