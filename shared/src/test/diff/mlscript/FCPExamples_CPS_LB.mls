// ------------ Dummy classes to represent the types in the examples ------------

class List[a]
  method Get: a
//│ Defined class List[+a]
//│ Declared List.Get: List['a] -> 'a

// Used to represent `::`
def cons: 'a -> List['a] -> List['a]
//│ cons: 'a -> List['a] -> List['a]

// Used to represent `[]`
def nil: List['a]
//│ nil: List[nothing]

def concat: string -> List[string] -> string
//│ concat: string -> List[string] -> string


// ------------ Examples ------------


// (* s  starts a sequence, cps style *)
def s k = k () nil
//│ s: (() -> List[nothing] -> 'a) -> 'a

def insert v () acu k = k () (cons v acu)
//│ insert: 'a -> () -> (forall 'a. List['a] -> (forall 'a, 'b. (() -> List['a] -> 'b) -> 'b))

// (* x  inserts "x" in the acu *)
def x () = insert "x" ()
//│ x: () -> (forall 'a. List['a] -> (forall 'a, 'b. (() -> List['a] -> 'b) -> 'b))

// (* We call x a 'token', in the sequence. *)

// (* e  ends the sequence. It builds a token that can be inserted in another sequence or that can be printed. *)
def e () acu = insert (concat "," acu)
//│ e: () -> List[string] -> () -> (forall 'a. List['a] -> (forall 'a, 'b. (() -> List['a] -> 'b) -> 'b))


// (* Prints a token *)
def print t = t () nil (fun () -> fun r -> log (concat "," r))
//│ print: (() -> List[nothing] -> (() -> List[string] -> unit) -> 'a) -> 'a

// Added by me:
s x x (s x x e)
//│ res: (() -> List[nothing] -> 'a) -> 'a


def test8  = (s x x (s x x e) x (s x x x e) e)
//│ test8: () -> (forall 'a. List['a] -> (forall 'a, 'b. (() -> List['a] -> 'b) -> 'b))

print test8


def test14 = (s x x x x x x x x x x x x x x e)
//│ test14: () -> (forall 'a. List['a] -> (forall 'a, 'b. (() -> List['a] -> 'b) -> 'b))

def test16 = (s x x x x x x x x x x x x x x x x e)
//│ test16: () -> (forall 'a. List['a] -> (forall 'a, 'b. (() -> List['a] -> 'b) -> 'b))

// (* This is too much for the type-checker. *)
def test18 = (s x x x x x x x x x x x x x x x x x x e)
//│ test18: () -> (forall 'a. List['a] -> (forall 'a, 'b. (() -> List['a] -> 'b) -> 'b))


// (* A function that receives a token *)
def f t = (s x x t x x e)
//│ f: (() -> List[nothing] -> (() -> (forall 'a. List['a] -> (forall 'a, 'b. (() -> List['a] -> 'b) -> 'b))) -> (() -> (forall 'a. List['a] -> (forall 'a, 'b. (() -> List['a] -> 'b) -> 'b))) -> (() -> List[string] -> () -> (forall 'a. List['a] -> (forall 'a, 'b. (() -> List['a] -> 'b) -> 'b))) -> 'c) -> 'c

// (* If the token is used twice, we must reveive two arguments *)
def g t1 t2 = (s x x t1 x (s x t2 x e) e)
//│ g: (() -> List[nothing] -> (() -> (forall 'a. List['a] -> (forall 'a, 'b. (() -> List['a] -> 'b) -> 'b))) -> 'c -> (() -> List[string] -> () -> (forall 'a. List['a] -> (forall 'a, 'b. (() -> List['a] -> 'b) -> 'b))) -> 'd) -> (forall 'c, 'd. (() -> List[nothing] -> (() -> (forall 'a. List['a] -> (forall 'a, 'b. (() -> List['a] -> 'b) -> 'b))) -> (() -> List[string] -> () -> (forall 'a. List['a] -> (forall 'a, 'b. (() -> List['a] -> 'b) -> 'b))) -> 'c) -> 'd)

// (* This does not type. It requires first-class polymorphism. *)
def h t = g t t
//│ h: (() -> List[nothing] -> (() -> (forall 'a. List['a] -> (forall 'a, 'b. (() -> List['a] -> 'b) -> 'b))) -> (() -> List[string] -> () -> (forall 'a. List['a] -> (forall 'a, 'b. (() -> List['a] -> 'b) -> 'b))) -> anything & () -> List[nothing] -> (() -> (forall 'a. List['a] -> (forall 'a, 'b. (() -> List['a] -> 'b) -> 'b))) -> nothing -> (() -> List[string] -> () -> (forall 'a. List['a] -> (forall 'a, 'b. (() -> List['a] -> 'b) -> 'b))) -> anything) -> nothing

