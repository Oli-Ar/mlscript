    Lit -> e.val
    }
//│ evalStub1: (Lit with {val: 'val}) -> 'val
//│          = [Function: evalStub1]

rec def evalStub2 e = case e of {
    Lit -> e.val | Nega -> 0 - (evalStub2 e.arg)
    }
//│ evalStub2: 'a -> int
//│   where
//│     'a <: Lit | Nega[?] & {arg: 'a}
