class Lit: {val: int}
class Add[E]: { lhs: E; rhs: E }
class Nega[E]: { arg: E}
def lit val = Lit { val }
def add lhs rhs = Add { lhs; rhs }
def nega arg = Nega { arg }
//│ Defined class Lit
//│ Defined class Add[+E]
//│ Defined class Nega[+E]
//│ lit: (int & 'val) -> (Lit with {val: 'val})
